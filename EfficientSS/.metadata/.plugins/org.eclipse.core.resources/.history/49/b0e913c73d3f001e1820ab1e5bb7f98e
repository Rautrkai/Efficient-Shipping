package com.app.EfficientSS.service;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.http.ResponseEntity;
import org.springframework.stereotype.Service;

import com.app.EfficientSS.beans.Customer;
import com.app.EfficientSS.beans.Feedback;
import com.app.EfficientSS.dao.FeedbackDao;

@Service
public class FeedbackServiceImpl implements FeedbackService  {

	@Autowired
	private FeedbackDao feedbackDao;
	
	private CustomerDao  customerDao;
	@Override
	public ResponseEntity<String> registerCustFeedback(Feedback feedback, int cust_id) {
		
		Customer cust= 
		return feedbackDao.registerCustFeedback(feedback,cust_id);
	}

}
