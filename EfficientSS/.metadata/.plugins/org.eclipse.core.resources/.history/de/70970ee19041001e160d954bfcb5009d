		try {
			int tranid=Integer.parseInt(t_id);
		    String str= URLEncoder.encode(item_Id, "UTF-8");
		    int itemid=Integer.parseInt(item_Id);
		    
		} catch (UnsupportedEncodingException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	 
	    return "555";
	}

}
